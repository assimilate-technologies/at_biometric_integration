{
    "report_name": "Attendance Regularization Report",
    "name": "Attendance Regularization Report",
    "module": "at_biometric_integration",
    "doctype": "Attendance",
    "filters": [
        {
            "fieldname": "from_date",
            "label": "From Date",
            "fieldtype": "Date",
            "default": "Today",
            "reqd": 1
        },
        {
            "fieldname": "to_date",
            "label": "To Date",
            "fieldtype": "Date",
            "default": "Today",
            "reqd": 1
        },
        {
            "fieldname": "employee",
            "label": "Employee",
            "fieldtype": "Link",
            "options": "Employee",
            "reqd": 0
        },
        {
            "fieldname": "status",
            "label": "Status",
            "fieldtype": "Select",
            "options": "Draft\nPending\nApproved\nRejected",
            "reqd": 0
        },
        {
            "fieldname": "reason",
            "label": "Reason",
            "fieldtype": "Small Text",
            "reqd": 0
        }
    ]
}